elated Features"],[478,"Related Features"],[235,"request - Request details (method, url, options)"],[317,"Request Cancelling"],[301,"Request debugging"],[406,"Request errors (malformed requests)"],[350,"Request Examples"],[209,"Request Hooks (onRequest)"],[245,"Request hooks allow you to modify the request before it's sent using helper methods:"],[131,"Request timeout in milliseconds"],[51,"Request-level options always take precedence over instance-level options."],[29,"Request-Level Overrides"],[22,"Request/Response Processing"],[307,"Resource existence checks"],[446,"Respects withCredentials: sets xhr.withCredentials = true when configured"],[210,"Response Hooks (onResponse)"],[246,"Response hooks allow you to modify the response before it's returned:"],[24,"Response Processing"],[236,"result - Response result (null during onRequest)"],[134,"retry"],[14,"Retry Configuration"],[319,"Retry Logic"],[390,"Retry Logic with Error Handling"],[371,"Returns a result object with data, error, and native response"],[154,"revalidateCache"],[303,"Security testing"],[63,"See Instance Options for a list of available options."],[299,"Server feature detection"],[475,"Server-Sent Events (SSE) Alternative"],[32,"Set a base URL for all requests:"],[124,"Set a common base URL for all API requests"],[140,"Set a limit on the number of retries"],[132,"Set a timeout for requests"],[196,"Set cache mode ('default', 'no-store', 'reload', etc.)"],[176,"Set CORS mode (e.g., 'cors', 'no-cors')"],[192,"Set credentials mode ('include', 'same-origin', 'omit')"],[180,"Set default headers"],[33,"Set default headers for all requests:"],[31,"Set default options when creating an instance:"],[457,"Set default progress handlers for all requests:"],[229,"Set hooks when creating your instance"],[152,"Set polling interval"],[39,"Set request timeout in milliseconds:"],[342,"Set request timeouts with automatic error handling:"],[240,"setBody() - Update request body"],[244,"setError() - Update error information